{
    "name": "vasildakov/postcode-validator",
    "description": "UK Postcode Validator",
    "keywords": [
        "uk",
        "postcode",
        "validator",
        "post",
        "php"
    ],
    "homepage": "https://github.com/vasildakov/postcode-validator",
    "license": "MIT",
    "authors": [
        {
            "name": "Vasil Dakov",
            "email": "vasildakov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7|^5.0",
        "squizlabs/php_codesniffer": "~2.3",
        "satooshi/php-coveralls": "~1.0",
        "phpunit/php-code-coverage": "^3.0|^4.0"
    },
    "suggest": {
        "vasildakov/postcode": "For creating Postcode value object",
        "vasildakov/postcode-parser": "For parsing postcodes",
        "vasildakov/postcode-doctrine": "For integration postcode with Doctrine ORM"
    },
    "autoload": {
        "psr-4": {
            "VasilDakov\\Postcode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VasilDakov\\Postcode\\": "tests/"
        }
    }
}
